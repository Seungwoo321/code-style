{
  "branches": ["main"],
  "tagFormat": "@seungwoo321/prettier-config@${version}",
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "parserOpts": {
          "headerPattern": "^(\\w*)\\(([\\w\\-]+)\\):\\s(.*)$",
          "headerCorrespondence": ["type", "scope", "subject"]
        },
        "releaseRules": [
          {
            "type": "feat",
            "scope": "prettier-config",
            "release": "minor"
          },
          {
            "type": "fix",
            "scope": "prettier-config",
            "release": "patch"
          },
          {
            "type": "docs",
            "scope": "prettier-config",
            "release": "patch"
          },
          {
            "type": "style",
            "scope": "prettier-config",
            "release": "patch"
          },
          {
            "type": "refactor",
            "scope": "prettier-config",
            "release": "patch"
          },
          {
            "type": "perf",
            "scope": "prettier-config",
            "release": "patch"
          },
          {
            "type": "test",
            "scope": "prettier-config",
            "release": "patch"
          },
          {
            "type": "build",
            "scope": "prettier-config",
            "release": "patch"
          },
          {
            "type": "ci",
            "scope": "prettier-config",
            "release": "patch"
          },
          {
            "type": "chore",
            "scope": "prettier-config",
            "release": "patch"
          }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "angular",
        "parserOpts": {
          "headerPattern": "^(\\w*)\\(([\\w\\-]+)\\):\\s(.*)$",
          "headerCorrespondence": ["type", "scope", "subject"]
        },
        "writerOpts": {
          "commitsSort": ["scope", "subject"]
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "."
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["package.json", "CHANGELOG.md"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    [
      "@semantic-release/github",
      {
        "successComment": "üéâ Ïù¥ PRÏùÄ [${nextRelease.version}](https://github.com/Seungwoo321/code-style/releases/tag/@seungwoo321/prettier-config@${nextRelease.version})Ïóê Ìè¨Ìï®ÎêòÏóàÏäµÎãàÎã§.",
        "failTitle": "semantic-release Ïã§Ìå®"
      }
    ]
  ]
}
